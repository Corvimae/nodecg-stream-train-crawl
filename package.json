    "styled-components": "^5.3.5",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "concurrently": "^7.3.0",
    "parcel-bundler": "^1.12.5",
    "typescript": "^4.9.5"
  }
